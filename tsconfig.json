{
  "compilerOptions": {
    "target": "es2017",
    "module": "esnext",
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react-jsx",
    "lib": ["esnext", "dom"],
    "allowJs": false,
    "checkJs": false,
    "baseUrl": ".",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "moduleResolution": "node",
    "isolatedModules": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "noFallthroughCasesInSwitch": true
  },
  "include": ["src"]
}
